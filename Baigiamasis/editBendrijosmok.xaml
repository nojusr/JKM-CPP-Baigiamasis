<Window x:Class="Baigiamasis.editBendrijosmok"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Komunaliniai mokesčiai" Height="485" Width="636" FontFamily="/Baigiamasis;component/Resources/#IBM Plex Sans Light">
    <Grid>
        <ComboBox x:Name="ben_ob" Margin="113,10,10,0" DropDownClosed="ben_ob_DropDownClosed_1" Height="22" VerticalAlignment="Top"/>
        <Label Content="Pasirinkti objektą:" HorizontalAlignment="Left" Margin="4,8,0,0" VerticalAlignment="Top" Width="123"/>
        <ListBox x:Name="ben_list" Margin="10,39,10,75" SelectionMode="Multiple"/>
        <Button Content="Pridėti bendrijos mokestį..." Margin="0,0,10,36" HorizontalAlignment="Right" Width="164" Height="20" VerticalAlignment="Bottom" Click="addBendrMok"/>
        <TextBox x:Name="ben_name" Margin="10,0,179,34" TextWrapping="Wrap" TextDecorations="{x:Null}" RenderTransformOrigin="0.5,0.5" Height="23" VerticalAlignment="Bottom" />
        <Button Content="Naikinti pasirinktas eilutes..." Margin="0,0,10,9" HorizontalAlignment="Right" Width="164" Height="20" VerticalAlignment="Bottom" Click="delBendrMok"/>
        <Label Content="Mokesčio pavadinimas:" Margin="4,0,0,49" FontSize="9" Height="26" VerticalAlignment="Bottom" HorizontalAlignment="Left" Width="201"/>

    </Grid>
</Window>
